{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "object-curly-newline": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "no-cond-assign": 0,
    "no-restricted-syntax" : 0,
    "no-await-in-loop": 0
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  }
}
